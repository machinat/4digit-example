{
  "name": "4digits-example",
  "private": true,
  "scripts": {
    "clean": "rm -rf ./lib ./dist && rm -f tsconfig.tsbuildinfo",
    "migrate": "per-env",
    "migrate:development": "dotenv -- ts-node ./src/cli/migrate.ts",
    "migrate:production": "node ./lib/cli/migrate.js",
    "build": "npm run clean && npm run build:source && npm run build:next",
    "build:source": "tsc",
    "build:next": "dotenv -- next build ./webview",
    "start": "per-env",
    "start:development": "dotenv -- ts-node ./src/index.ts",
    "start:production": "node ./lib/index.js",
    "dev": "dotenv -- ts-node ./src/cli/dev.ts"
  },
  "dependencies": {
    "@machinat/core": "^0.5.0-beta.36",
    "@machinat/dev-tools": "^0.5.0-beta.36",
    "@machinat/dialogflow": "^0.5.0-beta.36",
    "@machinat/http": "^0.5.0-beta.36",
    "@machinat/line": "^0.5.0-beta.36",
    "@machinat/messenger": "^0.5.0-beta.36",
    "@machinat/per-env": "^1.1.0",
    "@machinat/redis-state": "^0.5.0-beta.36",
    "@machinat/script": "^0.5.0-beta.36",
    "@machinat/stream": "^0.5.0-beta.36",
    "@machinat/telegram": "^0.5.0-beta.36",
    "@machinat/webview": "^0.5.0-beta.36",
    "commander": "^7.2.0",
    "next": "^12.0.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "umzug": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.2",
    "dotenv-cli": "^4.1.1",
    "localtunnel": "^2.0.1",
    "nodemon": "^2.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  }
}
